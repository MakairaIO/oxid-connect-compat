{
    "name": "makaira/oxid-connect-compat",
    "description": "Make makaira/connect compatible with oxid < 6.2",
    "type": "library",
    "require": {
        "kore/data-object": "1.2",
        "symfony/dependency-injection": "^2.8|^3.1",
        "symfony/config": "^2.8|^3.1",
        "symfony/yaml": "^2.8|^3.1|^4.0",
        "symfony/finder": "^2|^3",
        "symfony/expression-language": "^2.8|^3.4",
        "doctrine/dbal": "^2.5",
        "symfony/event-dispatcher": "^2.8|^3.1",
        "symfony/console": "^2.8|^3.1"
    },
    "conflict": {
        "oxid-esales/oxideshop-metapackage-ce": ">=6.2.0",
        "makaira/oxid-connect-essential": "*"
    },
    "autoload": {
        "psr-4": {
            "Makaira\\ConnectCompat\\": ["src/Makaira/ConnectCompat"]
        }
    },
    "authors": [
        {
            "name": "Martin Schnabel"
        }, {
            "name": "Michael Wille",
            "email": "mw@marmalade.group"
        }, {
            "name": "Stefan Krenz",
            "email": "sk@marmalade.group"
        }
    ],
    "bin": ["bin/makaira-connect"]
}
